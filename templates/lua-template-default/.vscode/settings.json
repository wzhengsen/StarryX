{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "build/": true,
        "build-mac": true,
        "build-ios": true,
        "build-simulator": true,
        "**/.gitmodules": true,
        "GPUCache": true,
        "frameworks/runtime-src/proj.android/app/build": true,
        "frameworks/runtime-src/proj.android/app/assets": true,
        "frameworks/runtime-src/proj.android/app/.cxx": true,
        "frameworks/runtime-src/proj.android/app/.externalNativeBuild": true,
        "frameworks/runtime-src/proj.android/.idea": true,
        "frameworks/runtime-src/proj.android/.gradle": true
    },
    "files.trimTrailingWhitespace": true,
    "files.eol": "\n",
    "files.autoGuessEncoding": true,
    "editor.autoClosingQuotes": "never",
    "editor.autoClosingBrackets": "never",
    "lua_analyzer.codeLinting.checkWhileTyping": false,
    "lua_analyzer.codeLinting.enable": false,
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "lua_analyzer.codeLinting.checkAfterSave": false,
    "cmake.configureOnOpen": false,
    "Lua.diagnostics.globals": [
        "Event",
        "cc",
        "ccui",
        "ccb",
        "gl",
        "LuaObjcBridge",
        "LuaJavaBridge",
        "ccs",
        "sol",
        "class",
        "event",
        "enum",
        "ccu"
    ],
    "Lua.runtime.version": "Lua 5.4",
    "Lua.workspace.ignoreDir": [
        ".vscode",
        "tool",
        "frameworks",
        "build"
    ],
    "cmake.configureOnEdit": false,
    "Lua.workspace.preloadFileSize": 500
}